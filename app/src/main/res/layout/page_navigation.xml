<?xml version="1.0" encoding="utf-8"?>
<merge
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/black"
    tools:parentTag="androidx.constraintlayout.widget.ConstraintLayout"
    >

  <ImageView
      android:id="@+id/ivBack"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginEnd="8dp"
      android:src="@drawable/ic_baseline_arrow_back_24"
      app:layout_constraintBottom_toBottomOf="@id/tvPage"
      app:layout_constraintEnd_toStartOf="@id/tvPage"
      app:layout_constraintTop_toTopOf="@id/tvPage"

      />

  <ImageView
      android:id="@+id/ivForward"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginStart="8dp"
      android:src="@drawable/ic_baseline_arrow_forward_24"
      app:layout_constraintBottom_toBottomOf="@id/tvPage"
      app:layout_constraintStart_toEndOf="@id/tvPage"
      app:layout_constraintTop_toTopOf="@id/tvPage"
      />

  <ImageView
      android:id="@+id/ivExportBtn"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginEnd="16dp"
      android:src="@drawable/ic_baseline_ios_share_24"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      />

  <TextView
      android:id="@+id/tvSearchedResults"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:textColor="@color/white"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      />

  <TextView
      android:id="@+id/tvPage"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:textColor="@color/white"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      />

</merge>
